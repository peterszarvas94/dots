{
  "name": "prettier-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^3.0.3",
    "prettier-plugin-go-template": "^0.0.15",
    "prettier-plugin-tailwindcss": "^0.5.6"
  },
  "prettier": {
    "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-go-template"],
    "overrides": [
      {
        "files": "*.svg",
        "options": {
          "parser": "html"
        }
      },
      {
        "files": ["*.html"],
        "options": {
          "parser": "go-template"
        }
      }
    ],
    "tabWidth": 2,
    "goTemplateBracketSpacing": false
  }
}

